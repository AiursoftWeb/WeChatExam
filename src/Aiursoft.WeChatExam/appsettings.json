{
  "ConnectionStrings": {
    "AllowCache": "True",
    "DbType": "Sqlite",
    "DefaultConnection": "DataSource=app.db;Cache=Shared"
  },
  "Storage": {
    "Path": "/tmp/data"
  },
  "SenparcWeixinSetting": {
    "IsDebug": true,
    "DefaultCacheNamespace": "DefaultWeixinCache",
    "WeixinAppId": "your-app-id",
    "WeixinAppSecret": "your-app-secret",
    "Items": {}
  },
  "AppSettings": {
    "WechatAppId": "your-app-id",
    "WechatAppSecret": "your-app-secret"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}